{
  "name": "octopusteam/waapi-laravel",
  "description": "WAAPI whatsapp api integration for Laravel",
  "keywords": [
    "laravel",
    "waapi",
    "whatsapp",
    "api"
  ],
  "homepage": "https://github.com/octopus-software-team/waapi-laravel",
  "support": {
    "issues": "https://github.com/octopus-software-team/waapi-laravel/issues",
    "source": "https://github.com/octopus-software-team/waapi-laravel"
  },
  "authors": [
    {
      "name": "Octopus Team",
      "email": "octopussoftwareteam@gmail.com"
    },
    {
      "name": "Abdallah Mahmoud",
      "email": "abdallahmahmoud1422003@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload-dev": {
    "psr-4": {
      "OctopusTeam\\Waapi\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/octopus-software-team"
    }
  ],
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "OctopusTeam\\Waapi\\": "src/"
    }
  },
  "require": {
    "php": ">=8.0",
    "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "OctopusTeam\\Waapi\\WaapiServiceProvider"
      ],
      "aliases": {
        "Waapi": "OctopusTeam\\Waapi\\Facades\\Waapi"
      }
    }
  }
}
